{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "cstockdill/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "cstockdill/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "cstockdill/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "cstockdill/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMapping": [
        {
        "hostPort": 80,
        "containerPort": 80
         }
        ],
       "links": ["client", "server"]
      }
    ]
}
